/* pages/shoppingCard/shoppingCard.wxss */

.productListTop{
    height: 60rpx;
    width: 750rpx;
    background-color: #fff;
    padding: 14rpx;
    border-bottom: 1px solid #e5e5e5;
}

.spLogo{
     background: url("../../images/gouwulandaihong.png") no-repeat;
     background-size: contain;
     width: 68rpx;
     height: 60rpx;
    position: relative;
}

.spNum{
    font-size: 22rpx;
    color: #fff;
    position: absolute;
    right: 14rpx;
    top: 8rpx;
}

.productText
{
    width: 200rpx;
    height: 60rpx;
    font-size: 30rpx;
    margin-left: 10rpx;
    
}

.searchLogo{
  background: url("../../images/search.png") no-repeat;
  width: 32rpx;
  height: 32rpx;
  background-size: contain;
  margin-top: 12rpx;
  margin-left: 12rpx;
}

.productListSearchBox{
    width: 400rpx;
    height: 60rpx;
    background-color: #f5f5f5;
    border-radius: 4px;
}

.searchInput{
    margin-top: 6rpx;
    width: 320rpx;
    height: 20rpx;
    line-height: 32rpx;
    font-size: 26rpx;
    margin-left: 10rpx;
}

.top_text{
    width: 290rpx;
    height: 60rpx;
    margin-left: 24rpx;
}

.emsPrice{
    font-size: 22rpx;
    line-height: 30rpx;
    height: 30rpx;
}

.priceP{
    color: #cba461;
    font-size: 30rpx;
    line-height: 30rpx;
    height: 30rpx;
}

.productMenuImg{
    width: 150rpx;
    height: 150rpx;
}

.shoppingCardList{
    width: 100%;
    height: 190rpx;
   
  
}

.shoppingCardListWrap{
    width: 690rpx;
    height: 150rpx;
    padding:20rpx 0;
    margin: 0 auto;
    border-bottom: 2rpx dashed #cba461;
}

.menu_content{
    width: 400rpx;
    overflow: hidden;
    margin-left: 30rpx;
    position: relative;
}

.menO{
    font-size: 30rpx;
    color: #000;

}

.shopName{
    width: 60%;
    overflow: hidden;
    text-decoration: none;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.maiyisongyi{
    width: 120rpx;
    border: 2rpx solid #cba461;
    color: #cba461;
    margin-left: 20rpx;
    font-size: 24rpx;
    text-align: center;
    height:30rpx;
    line-height: 30rpx;
}

.menuT{
    overflow: hidden;
    text-decoration: none;
    text-overflow: ellipsis;
    white-space: nowrap;
    width: 400rpx;
    color: #bbb;
    line-height: 30rpx;
    margin-bottom: 16rpx;
    font-size: 26rpx;
}

.menuM{
    color: #cba461;
    font-size: 30rpx;
}

.menuK{
    color: #bbb;
    font-size: 28rpx;
}

.price{
    font-size: 40rpx;
}

.control{
    position: absolute;
    right: 0;
    bottom: 0;
    font-size:28rpx;
    line-height: 33rpx;
}
.addlogo{
    width: 33rpx;
    height: 33rpx;
    background: url("../../images/jiadianji.png") no-repeat;
    background-size: contain;
}

.mainColor{
    color: #cba461;
    margin: 0 10rpx;
}

.dellogo{
    width: 33rpx;
    height: 33rpx;
    background: url("../../images/jianse.png") no-repeat;
    background-size: contain;
}

.sC_btn{
    width: 100%;
    height: 90rpx;
    text-align: center;
    color: #fff;
    background-color: #cba461;
    font-size: 34rpx;
    line-height: 90rpx;
    position: fixed;
    bottom: 0px;
    left: 0px;
}
